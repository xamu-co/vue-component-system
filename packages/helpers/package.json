{
  "name": "@open-xamu-co/components-helpers",
  "version": "0.0.1",
  "description": "Xamu shared components helpers",
  "author": "@xamu-co",
  "main": "dist/index.js",
  "exports": {
    ".": {
      "node": {
        "module": "./dist/esm/index.js",
        "require": "./dist/index.js",
        "import": "./wrapper.mjs"
      },
      "default": "./dist/esm/index.js"
    },
    "./package.json": "./package.json"
  },
  "module": "./dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "files": [
    "CHANGELOG.md",
    "README.md",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xamu-co/components.git"
  },
  "keywords": [
    "ts",
    "js",
    "helpers"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xamu-co/components/issues"
  },
  "homepage": "https://github.com/xamu-co/components#readme",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "build:esm": "tsc",
    "build:cjs": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline",
    "build": "rimraf dist && npm run build:esm && npm run build:cjs"
  },
  "dependencies": {
    "core-js": "^3.24.1",
    "interpolate-string": "^1.3.0",
    "lodash": "^4.17.21",
    "sweetalert2": "^11.7.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.6",
    "@babel/core": "^7.22.8",
    "@babel/preset-env": "^7.22.7",
    "@babel/preset-typescript": "^7.22.5",
    "@types/lodash": "^4.14.192"
  },
  "bundledDependencies": [
    "@open-xamu-co/components-types"
  ],
  "browserslist": [
    "defaults"
  ]
}