{
	"name": "@open-xamu-co/vue-component-system",
	"version": "0.0.1",
	"description": "Vue component system for the xamu project",
	"scripts": {
		"i": "yarn --frozen-lockfile",
		"check:lint": "eslint --ext .ts,.vue src",
		"check:format": "prettier {lib,src}/{*,**/*,**/**/*}.{ts,vue} --check",
		"build": "rimraf dist && vite build && vue-tsc",
		"dev": "vite dev"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/xamu-co/components.git"
	},
	"keywords": [
		"vue",
		"component",
		"system"
	],
	"author": "@xamu-co",
	"publishConfig": {
		"access": "public"
	},
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/xamu-co/components/issues"
	},
	"homepage": "https://github.com/xamu-co/components/packages/vue#readme",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"default": "./dist/index.js"
		},
