		"@open-xamu-co/common-types": "https://gitpkg.now.sh/xamu-co/common/packages/types?dev",
		"@types/uuid": "^9.0.1",
		"@vitejs/plugin-vue": "^4.2.1",
		"eslint-plugin-vue": "^9.12.0",
		"vue-tsc": "^1.6.4",
		"vite": "^4.4.2"
	},
	"optionalDependencies": {
		"@open-xamu-co/style-system": "https://gitpkg.now.sh/xamu-co/style-system?dev"
