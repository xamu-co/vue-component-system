{
	"name": "@open-xamu-co/vue-component-system",
	"version": "0.0.1",
	"description": "Vue component system for the xamu project",
	"main": "lib/index.mjs",
	"exports": {
		".": {
			"node": {
				"import": "./lib/index.mjs",
				"require": "./lib/index.umd.js"
			},
			"default": "./lib/index.mjs"
		},
		"./package.json": "./package.json",
		"./lib/style.css": {
			"import": "./lib/style.css",
			"require": "./lib/style.css"
		}
	},
	"style": "lib/style.css",
	"module": "lib/index.mjs",
	"types": "lib/index.d.ts",
	"files": [
		"CHANGELOG.md",
		"README.md",
		"lib"
	],
	"scripts": {
		"i": "yarn --frozen-lockfile",
		"check:lint": "eslint --ext .ts,.vue src",
		"check:format": "prettier src/{*,**/*,**/**/*}.{ts,vue} --check",
		"build": "rimraf lib && vite build && vue-tsc",
		"prepare": "npm run build"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/xamu-co/components.git"
	},
	"keywords": [
		"vue",
		"component",
		"system"
	],
	"author": "@xamu-co",
	"publishConfig": {
		"access": "public"
	},
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/xamu-co/components/issues"
	},
	"homepage": "https://github.com/xamu-co/components/packages/vue#readme",
	"devDependencies": {
		"@vitejs/plugin-vue": "^4.2.1",
		"@open-xamu-co/components-types": "*",
		"eslint-plugin-vue": "^9.12.0",
		"vite": "^4.3.5",
		"vue": "^3.2.47",
		"vue-tsc": "^1.6.4"
	},
	"peerDependencies": {
		"vue": "^3.2.47"
	},
	"engines": {
		"node": ">=16.19.0",
		"yarn": ">=1.22.4"
	}
}