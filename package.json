{
	"name": "@open-xamu-co/vue-component-system",
	"version": "0.0.1",
	"description": "Vue component system for the xamu project",
	"main": "lib/index.mjs",
	"exports": {
		".": {
			"node": {
				"import": "./lib/index.mjs",
				"require": "./lib/index.umd.js"
			},
			"default": "./lib/index.mjs"
		},
		"./package.json": "./package.json",
		"./lib/style.css": {
			"import": "./lib/style.css",
			"require": "./lib/style.css"
		}
	},
	"style": "lib/style.css",
	"module": "lib/index.mjs",
	"types": "lib/index.d.ts",
	"files": [
		"CHANGELOG.md",
		"README.md",
		"lib"
	],
	"scripts": {
		"i": "yarn --frozen-lockfile",
		"check:lint": "eslint --ext .ts,.vue src",
		"check:format": "prettier src/{*,**/*,**/**/*}.{ts,vue} --check",
		"build": "rimraf lib && vite build && vue-tsc",
		"prepare": "npm run build"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/xamu-co/vue-component-system.git"
	},
	"keywords": [
		"vue",
		"component",
		"system"
	],
	"author": "@xamu-co",
	"publishConfig": {
		"access": "public"
	},
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/xamu-co/vue-component-system/issues"
	},
	"homepage": "https://github.com/xamu-co/vue-component-system#readme",
	"devDependencies": {
		"@commitlint/cli": "^9.1.1",
		"@commitlint/config-conventional": "^9.1.1",
		"@fortawesome/fontawesome-common-types": "^6.4.0",
		"@semantic-release/changelog": "^5.0.1",
		"@semantic-release/git": "^9.0.0",
		"@types/lodash": "^4.14.192",
		"@types/node": "^18.14.0",
		"@types/uuid": "^9.0.1",
		"@typescript-eslint/eslint-plugin": "^5.39.0",
		"@typescript-eslint/parser": "^5.39.0",
		"@vitejs/plugin-vue": "^4.2.1",
		"autoprefixer": "^9.8.6",
		"cssnano": "^6.0.1",
		"cssnano-preset-advanced": "^4.0.7",
		"eslint": "^8.25.0",
		"eslint-config-prettier": "^8.1.0",
		"eslint-import-resolver-typescript": "^3.5.5",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-prettier": "^3.3.1",
		"eslint-plugin-vue": "^9.12.0",
		"husky": "^4.2.5",
		"lint-staged": "^10.2.11",
		"node-css-mqpacker": "^8.0.1",
		"postcss": "^8.2.10",
		"postcss-cli": "^8.3.1",
		"postcss-custom-properties": "^9.1.1",
		"postcss-merge-selectors": "vis97c/postcss-merge-selectors",
		"postcss-precision": "vis97c/postcss-precision",
		"postcss-sort-media-queries": "^3.8.9",
		"prettier": "^2.0.5",
		"rimraf": "^3.0.2",
		"sass": "1.32.10",
		"semantic-release": "^17.1.1",
		"sort-css-media-queries": "vis97c/sort-css-media-queries",
		"stylelint": "^15.6.1",
		"stylelint-config-prettier": "^8.0.2",
		"stylelint-prettier": "^1.1.2",
		"typescript": "^5.0.4",
		"vite": "^4.3.5",
		"vue": "^3.2.47",
		"vue-tsc": "^1.6.4"
	},
	"dependencies": {
		"interpolate-string": "^1.3.0",
		"lodash": "^4.17.21",
		"sweetalert2": "^11.7.5"
	},
	"peerDependencies": {
		"@open-xamu-co/common-enums": "*",
		"@open-xamu-co/common-helpers": "*",
		"@open-xamu-co/common-types": "*",
		"vue": "^3.2.47"
	},
	"engines": {
		"node": ">=16.19.0",
		"yarn": ">=1.22.4"
	},
	"husky": {
		"hooks": {
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
			"pre-commit": "yarn check --integrity && lint-staged"
		}
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"lint-staged": {
		"*.ts": [
			"prettier --write"
		],
		"*.vue": [
			"prettier --write"
		],
		"*.css": [
			"stylelint --fix",
			"prettier --write"
		],
		"*.scss": [
			"stylelint --syntax=scss --fix",
			"prettier --write"
		]
	},
	"stylelint": {
		"extends": [
			"stylelint-prettier/recommended"
		],
		"rules": {
			"indentation": null
		}
	},
	"browserslist": [
		"last 2 versions, > 0.5%, ie >= 11"
	]
}